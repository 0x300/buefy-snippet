{
    "dropdowns": {
        "prefix": "b-dropdown",
        "body": [
            "<!-- b-dropdown -->",
            "<b-dropdown>",
                "\t<button class=\"button is-primary\" slot=\"trigger\"",
                    "\t\t<span>Click me!</span>",
                    "\t\t<b-icon icon=\"arrow_drop_down\"></b-icon>",
                "\t</button>",

                "<b-dropdown-option>Action</b-dropdown-option>",
                "<b-dropdown-option>Another action</b-dropdown-option>",
                "<b-dropdown-option>Something else</b-dropdown-option>",
            "</b-dropdown>",
            "<!-- / b-dropdown END -->"
        ],
        "description": "cliked b-dropdown"
    },
    "dropdowns hoverable": {
        "prefix": "b-dropdown-hoverable",
        "body": [
            "<!-- b-dropdown hoverable -->",
            "<b-dropdown hoverable>",
                "\t<button class=\"button is-info\" slot=\"trigger\">",
                    "\t\t<span>Hover me!</span>",
                    "\t\t<b-icon icon=\"arrow_drop_down\"></b-icon>",
                "\t</button>",

                "\t<b-dropdown-option>Action</b-dropdown-option>",
                "\t<b-dropdown-option>Another action</b-dropdown-option>",
                "\t<b-dropdown-option>Something else</b-dropdown-option>",
            "</b-dropdown>",
            "<!-- / b-dropdown hoverable END -->"
        ],
        "description" : "dropdown hoverable"
    },
