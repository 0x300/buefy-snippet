    "dropdowns custom trigger": {
        "prefix": "b-dropdown-custom",
        "body": [
            "<!-- b-dropdown custom trigger-->",
            "<b-dropdown>",
                "\t<p class=\"tag is-success\" slot=\"trigger\">",
                    "\t\tCustom trigger",
                "\t</p>",
                "\t<b-dropdown-option>Action</b-dropdown-option>",
                "\t<b-dropdown-option>Another action</b-dropdown-option>",
                "\t<b-dropdown-option>Something else</b-dropdown-option>",
            "</b-dropdown>",
            "<!-- / b-dropdown custom trigger END -->"
        ],
        "description": "dropdown custom trigger"
    },
    "Menu simple": {
        "prefix": "b-menu",
        "body": [
            "<!-- b-menu -->",
            "<b-menu>",
                "\t<b-menu-list label=\"General\">",
                    "\t\t<b-menu-item>Dashboard</b-menu-item>",
                    "\t\t<b-menu-item>Customer</b-menu-item>",
                "\t</b-menu-list>",
                "\t<b-menu-list label=\"Administration\">",
                    "\t\t<b-menu-item>Team Settings</b-menu-item>",
                    "\t\t<b-menu-dropdown>",
                        "\t\t\t<template slot=\"trigger\">Manage Your Team</template>",
                        "\t\t\t<b-menu-item>Members</b-menu-item>",
                        "\t\t\t<b-menu-item>Plugins</b-menu-item>",
                        "\t\t\t<b-menu-item>Add a member</b-menu-item>",
                    "\t\t</b-menu-dropdown>",
                    "\t<b-menu-item>Invitations</b-menu-item>",
                    "\t<b-menu-item>Cloud Storage Environment Settings</b-menu-item>",
                    "\t<b-menu-item>Authentication</b-menu-item>",
                "\t</b-menu-list>",
                "\t<b-menu-list label=\"Transactions\">",
                    "\t\t<b-menu-item>Payments</b-menu-item>",
                    "\t\t<b-menu-item>Transfers</b-menu-item>",
                    "\t\t<b-menu-item>Balance</b-menu-item>",
                "\t</b-menu-list>",
            "</b-menu>",
            "<!-- / b-menu END -->"
        ],
        "description": "A simple menu, for any type of vertical navigation"
    },
    "menu multi-level": {
        "prefix": "b-menu-multi",
        "body": [
            "<!-- b-menu multi-level -->",
            "<b-menu>",
                "\t<b-menu-list>",
                    "\t\t<b-menu-dropdown>",
                        "\t\t\t<template slot=\"trigger\">Level 1</template>",
                        "\t\t\t<b-menu-dropdown>",
                            "\t\t\t\t<template slot=\"trigger\">Level 2</template>",
                            "\t\t\t\t<b-menu-dropdown>",
                                "\t\t\t\t\t<template slot=\"trigger\">Level 3</template>",
                                "\t\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                                "\t\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                            "\t\t\t\t</b-menu-dropdown>",
                            "\t\t\t\t<b-menu-dropdown>",
                                "\t\t\t\t\t<template slot=\"trigger\">Level 3</template>",
                                "\t\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                                "\t\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                            "\t\t\t\t</b-menu-dropdown>",
                        "\t\t\t</b-menu-dropdown>",
                        "\t\t<b-menu-dropdown>",
                            "\t\t\t<template slot=\"trigger\">Level 2</template>",
                            "\t\t\t<b-menu-dropdown>",
                                "\t\t\t\t<template slot=\"trigger\">Level 3</template>",
                                "\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                                "\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                            "\t\t\t</b-menu-dropdown>",
                            "\t\t\t<b-menu-dropdown>",
                                "\t\t\t\t<template slot=\"trigger\">Level 3</template>",
                                "\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                                "\t\t\t\t<b-menu-item>Level 4</b-menu-item>",
                            "\t\t\t</b-menu-dropdown>",
                        "\t\t</b-menu-dropdown>",
                    "\t\t</b-menu-dropdown>",
                "\t</b-menu-list>",
            "</b-menu>",
            "<!-- / b-menu multi level END -->"
        ],
        "description": "Menu con multi level"
    },
    "Panel": {
        "prefix": "b-panel",
        "body": [
            "<!-- b-panel -->",
            "<b-panel>",
                "\t<strong slot=\"header\">Title</strong>",
                "\t<div class=\"content\">",
                    "\t\t<h3>",
                        "\t\t\t<!-- Subtitle -->",
                    "\t\t</h3>",
                    "\t\t<p>",
                        "\t\t\t<!-- Content -->",
                    "\t\t</p>",
                "\t</div>",
            "</b-panel>",
            "<!-- / b-panel END -->"
        ],
        "description": "A simple panel"
    },
    "Panel tabs": {
        "prefix": "b-panel-tabs",
        "body": [
            "<!-- b-panel-tabs -->",
            "<b-panel :collapsible=\"true\" hasCustomTemplate header=\"Title\">",
                "\t<p class=\"panel-tabs\">",
                    "\t\t<a class=\"is-active\">All</a>",
                    "\t\t<a>Public</a>",
                    "\t\t<a>Private</a>",
                "\t</p>",
                "\t<div class=\"panel-block\">",
                    "\t\t<!-- content -->",
                "\t</div>",
            "</b-panel>",
            "<!-- / b-panel tabs END -->"
        ],
        "description": "A panel Tabs"
    },
    "tabs": {
        "prefix": "b-tabs",
        "body": [
            "<!-- b-tabs -->",
            "<b-tabs>",
                "\t<b-tab-item label=\"Tabs1\">",
                    "\t\t<!-- Content 1 -->",
                "\t</b-tab-item>",

                "\t<b-tab-item label=\"Tabs2\">",
                    "\t<!-- content 2 -->",
                "\t</b-tab-item>",
            "</b-tabs>",
            "<!-- / b-tabs END -->"
        ],
        "description": "Responsive horizontal navigation tabs"
    },
    "tabs centered": {
        "prefix": "b-tabs-center",
